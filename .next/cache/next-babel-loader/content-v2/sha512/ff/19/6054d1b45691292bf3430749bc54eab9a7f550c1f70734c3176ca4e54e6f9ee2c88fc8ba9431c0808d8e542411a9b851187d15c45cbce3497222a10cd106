\r\n  const [loading, setLoading] = useState(false);\r\n\r\n  const isFollowing =\r\n    loggedUserFollowStats.following.length > 0 &&\r\n    loggedUserFollowStats.following.filter(\r\n      following => following.user === profile.user._id\r\n    ).length > 0;\r\n\r\n  return (\r\n    <>\r\n      <Segment>\r\n        <Grid stackable>\r\n          <Grid.Column width={11}>\r\n            <Grid.Row>\r\n              <Header\r\n                as=\"h2\"\r\n                content={profile.user.name}\r\n                style={{ marginBottom: \"5px\" }}\r\n              />\r\n            </Grid.Row>\r\n\r\n            <Grid.Row stretched>\r\n              {profile.bio}\r\n              <Divider hidden />\r\n            </Grid.Row>\r\n\r\n            <Grid.Row>\r\n              {profile.social ? (\r\n                <List>\r\n                  <List.Item>\r\n                    <List.Icon name=\"mail\" />\r\n                    <List.Content content={profile.user.email} />\r\n                  </List.Item>\r\n\r\n                  {profile.social.facebook && (\r\n                    <List.Item>\r\n                      <List.Icon name=\"facebook\" color=\"blue\" />\r\n                      <List.Content\r\n                        style={{ color: \"blue\" }}\r\n                        content={profile.social.facebook}\r\n                      />\r\n                    </List.Item>\r\n                  )}\r\n\r\n                  {profile.social.instagram && (\r\n                    <List.Item>\r\n                      <List.Icon name=\"instagram\" color=\"red\" />\r\n                      <List.Content\r\n                        style={{ color: \"blue\" }}\r\n                        content={profile.social.instagram}\r\n                      />\r\n                    </List.Item>\r\n                  )}\r\n\r\n                  {profile.social.youtube && (\r\n                    <List.Item>\r\n                      <List.Icon name=\"youtube\" color=\"red\" />\r\n           