r\n}\r\n\r\nconst ResultRenderer = ({ _id, profilePicUrl, name }) => {\r\n  return (\r\n    <List key={_id}>\r\n      <List.Item>\r\n        <Image src={profilePicUrl} alt=\"ProfilePic\" avatar />\r\n        <List.Content header={name} as=\"a\" />\r\n      </List.Item>\r\n    </List>\r\n  );\r\n};\r\n\r\nexport default SearchComponent;\r\n"]},"metadata":{},"sourceType":"module"}